{
    "name": "devpark/laravel-przelewy24",
    "description": "Laravel przelewy24.pl integration",
    "type": "library",
    "require": {
        "laravel/framework": "5.*|6.*|7.*",
        "guzzlehttp/guzzle": "^6.2|6.3"
    },
    "autoload": {
        "psr-4": {
            "Devpark\\Transfers24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "mockery/mockery": "^0.9.6"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Devpark\\Transfers24\\Providers\\Transfers24ServiceProvider"
            ]
        }
    }
}
